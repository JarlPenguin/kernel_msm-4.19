# Disable unused qcom peripheral drivers
CONFIG_INPUT_HBTP_INPUT=n
CONFIG_QPNP_LINEAR_CHARGER=n
CONFIG_QPNP_QG=n
CONFIG_QPNP_SMB5=n
CONFIG_QPNP_VM_BMS=n
CONFIG_SMB1351_USB_CHARGER=n
CONFIG_SMB1355_SLAVE_CHARGER=n
CONFIG_SMB1360_CHARGER_FG=n

# Mach
CONFIG_MACH_MOTOROLA_MSM8937=y

# GPU
CONFIG_QCOM_KGSL=m

# Power
CONFIG_QPNP_SMBCHARGER=n
